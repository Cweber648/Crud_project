<h2>Rapper Name: <%= @artist.name %></h2>
<h3>Hails From: <%= @artist.hails_from  %></h3>
<h3>Label: <%= @artist.label %></h3>

<ul>
  <% @artist.albums.each do |album| %>
  <li>Album Title: <%= album.name %></li><br>
  <li>Release Date: <%= album.release_date %></li><br>
  <li>Executive Producer: <%= album.producer.name %></li><br><br>
  <% end %>
</ul>

